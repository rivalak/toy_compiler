// if
def foo(x)
    if x < 3 then
        1
    else
        foo(x-1) + foo(x-2)

// def & call
def foo1(x y)
x + y * 16;

foo1(5, 6);

// for
for i = 1, i < n, 1 in
    x + y;

// operator overloading
def binary | (LHS RHS)
if LHS then
1
else if RHS then
1
else
0;

def unary! (v)
	if v then
		0
	else
		1;
